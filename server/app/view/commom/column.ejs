<div class="nav-left">
  <div class="row-title">
    <div class="title-link"></div>
    <% const pid = ctx.params.pid %>
    <h5><%= menuList[pid].title %></h5>
  </div>
  <% if(menuList[pid].children){ %>
  <div class="nav-left-list">
    <ul>
      <% menuList[pid].children.forEach(item=>{ %>
      <li>
          <a href="<%= '/'+item.url+'/pid/'+[pid]+'/cid/'+item.id %>" class="<%= pid === ctx.params.pid && item.id == ctx.params.cid ? 'active' : '' %>"><%= item.title %></a>
        </li>
      <% }) %>
    </ul>
  </div>
  <% } %>
</div>