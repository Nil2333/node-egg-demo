<%- include('/commom/header',{
  title:servicesList?servicesList.title:"海南昆仑体育文化股份有限公司",
  Keywords:servicesList?servicesList.Keywords:"海南昆仑体育文化股份有限公司",
  companyDescription:servicesList?servicesList.companyDescription:"海南昆仑体育文化股份有限公司",
}) %>

<%- include('/commom/banner') %>

<div class="content-full">
  <div class="content main-info">

    <%- include('/commom/column') %>
      <div class="content-right">
        <div class="ser-top">
            <div class="ser-top-txt">
                <h5><%= servicesList[0].title %></h5>
                <p><%= servicesList[0].companyDescription %></p>
            </div>
            <img src="<%= servicesList[0].servicesImage %>" alt="" class="ser-top-img">
        </div>
        <%- include('/commom/serInfoPhoto') %>
        <div class="ser-info-text">
          <%- servicesList.content %>
        </div>
      </div>
  </div>
</div>
<%- include('/commom/serInfo') %>

<%- include('/commom/footer') %>
<script>
  var swiper = new Swiper('.ser-info-photo', {
    slidesPerView: 'auto',
    spaceBetween: 30,
    freeMode: true,
    pagination: {
        el: '.photo-pagination',
        clickable: true,
    },
  });
</script>
</body>
</html>